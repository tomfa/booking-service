{
  "name": "@booking-service/backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "rm -rf lambda/dist && tsc",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:watch": "yarn test --watch",
    "cdk": "cdk",
    "start:test": "npm run prisma:migrate && export NODE_ENV=test && ts-node ./local.ts",
    "start": "nodemon",
    "deploy": "npm run build && cdk deploy -O ../cdk-exports.json",
    "schema:generate-document": "gqlg --schemaFilePath ./graphql/schema.graphql --destDirPath ./graphql/generated/documents --depthLimit 5",
    "schema:generate": "npm run schema:generate-document && graphql-codegen --config codegen.yml",
    "prisma:generate-schema": "prisma validate && prisma introspect && prisma generate",
    "prisma:reset": "prisma migrate reset -f",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "export DATABASE_URL=postgresql://postgres:postgres_pass@localhost:5432/vailable_test && prisma migrate dev"
  },
  "prisma": {
    "schema": "lambda/prisma/schema.prisma"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.108.1",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-graphql-request": "^3.2.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@types/aws-lambda": "^8.10.77",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@types/node-jose": "^1.1.6",
    "@types/uuid": "^8.3.0",
    "apollo-boost": "^0.4.9",
    "aws-cdk": "1.108.1",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "gql-generator": "^1.0.12",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "jest": "^26.4.2",
    "nock": "^13.1.0",
    "node-jose": "^2.0.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.25.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "^1.108.1",
    "@aws-cdk/aws-certificatemanager": "^1.110.0",
    "@aws-cdk/aws-cloudfront": "^1.110.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.110.0",
    "@aws-cdk/aws-ec2": "^1.108.1",
    "@aws-cdk/aws-iam": "^1.108.1",
    "@aws-cdk/aws-lambda": "^1.108.1",
    "@aws-cdk/aws-rds": "^1.108.1",
    "@aws-cdk/core": "1.108.1",
    "@prisma/client": "^2.24.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "source-map-support": "^0.5.16",
    "uuid": "^8.3.2"
  }
}
