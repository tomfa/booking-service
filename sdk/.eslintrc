{
  "env": { "browser": true, "jest": true, "jasmine": true },
  "extends": ["@askeladden/eslint-config-askeladden/typescript-recommended"],
  "rules": {
    // Prettier handles
    "@typescript-eslint/indent": 0,
  },
  "settings": {
    "import/ignore": ["@booking-service/shared"],
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
