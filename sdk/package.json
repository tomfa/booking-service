{
  "name": "vailable",
  "version": "0.0.0-development",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "description": "Shared types and utils for web and api",
  "dependencies": {
    "tslib": "^2.3.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-graphql-request": "^3.2.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.10",
    "dotenv": "^10.0.0",
    "gql-generator": "^1.0.12",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "semantic-release": "^17.4.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "npm run schema:generate && tsc -pretty -p .",
    "test": "jest --runInBand --passWithNoTests",
    "test:watch": "jest --runInBand  --watch",
    "release": "semantic-release",
    "schema:generate-document": "gqlg --schemaFilePath ../api/schema.graphql --destDirPath ./src/graphql/generated/documents --depthLimit 5",
    "schema:generate": "npm run schema:generate-document && graphql-codegen --config codegen.yml",
    "predeploy": "npm run schema:generate && npm run build"
  },
  "author": "Tomas Albertsen <tomas@webutvikling.org> (http://webutvikling.org/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomfa/booking-service.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
