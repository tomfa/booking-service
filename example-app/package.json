{
  "name": "vailable-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run schema:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "schema:generate-document": "gqlg --schemaFilePath ../api/schema.graphql --destDirPath ./graphql/generated/documents --depthLimit 5",
    "schema:generate": "npm run schema:generate-document && graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "graphql": "^15.5.3",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.39.0",
    "vailable": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^2.1.3",
    "@graphql-codegen/typescript-react-apollo": "^3.1.3",
    "@types/react": "17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "gql-generator": "^1.0.12",
    "typescript": "4.4.2"
  }
}
