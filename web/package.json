{
  "name": "@booking-service/web",
  "version": "1.0.0",
  "description": "NextJS app demonstrating use of PDF API",
  "engines": {
    "node": ">=12.18.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@booking-service/shared": "1.0.0",
    "graphql": "^15.5.1",
    "next": "^10.0.9",
    "next-auth": "^3.27.3",
    "phosphor-react": "^1.2.1",
    "react": "^17.0.1",
    "react-alert": "^7.0.2",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@graphql-codegen/typescript-react-apollo": "^3.0.0",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-alert": "^5.2.0",
    "@types/styled-components": "^5.1.9",
    "gql-generator": "^1.0.12",
    "jest": "^26.6.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "next dev -p 8000",
    "start:production": "next start -p 5000",
    "build": "npm run schema:generate && next build",
    "export": "next export",
    "schema:generate-document": "gqlg --schemaFilePath ../api/schema.graphql --destDirPath ./graphql/generated/documents --depthLimit 5",
    "schema:generate": "npm run schema:generate-document && graphql-codegen --config codegen.yml",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "predeploy": "npm run schema:generate && npm run build && npm run export"
  },
  "author": "",
  "license": "ISC"
}
